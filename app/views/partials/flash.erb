<% if flash[:errors] %>
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <% flash[:errors].each do |error| %>
        <div class="alert alert-danger">
          <%= error %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
  <% end %>
