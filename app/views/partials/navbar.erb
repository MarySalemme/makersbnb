<% if current_user %>
<div class="topnav" id="myTopnav">
  <a href="/">Home</a>
  <a href="/users/spaces">My spaces</a>
  <a href="/space/new">Add a space</a>
  <a href="/space">Find a space</a>
  <div>
    <form id="sign_out" method="post" action="/sessions">
      <input type="hidden" name="_method" value="delete">
      <input type="submit" value="Sign Out">
    </form>
  </div>
  <div>
    <p id="signed_in"> Signed in as <%= current_user.username %></p>
  </div>
</div>
<% else %>
<div class="topnav" id="myTopnav">
  <a href="/">Home</a>
  <a href="/sessions/new">Log in</a>
  <a href="/users/new">New user?</a>
</div>
<% end %>
